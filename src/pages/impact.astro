---
import { SwitchSurveyPage } from '@components/pages'
import BaseLayout from '@layouts/BaseLayout.astro'
import { getByUIDSafe } from '@lib/prismic'

// SwitchSurveyPage uses textonlypages with UID "impactpage"
const page = await getByUIDSafe('textonlypages', 'impactpage')
---

<BaseLayout title="Switch Survey">
  <SwitchSurveyPage page={page} client:load />
</BaseLayout>
