---
import { ImpactPage } from '@components/pages'
import BaseLayout from '@layouts/BaseLayout.astro'
import { getByUIDSafe } from '@lib/prismic'

// Impact page uses textonlypages with UID "impactpage"
const page = await getByUIDSafe('textonlypages', 'impactpage')
---

<BaseLayout title="Switch Survey">
  <ImpactPage page={page} client:load />
</BaseLayout>
