---
import BaseLayout from "@layouts/BaseLayout.astro";
import { getByUIDSafe } from "@lib/prismic";
import { NotFoundPage } from "@components/pages";

const page = await getByUIDSafe("textonlypages", "notfound");
---

<BaseLayout title="Page Not Found">
  <NotFoundPage page={page} client:load />
</BaseLayout>
