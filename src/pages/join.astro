---
import { JoinPage } from '@components/pages'
import BaseLayout from '@layouts/BaseLayout.astro'
import { getByUIDSafe } from '@lib/prismic'

// Join page uses textonlypages with UID "joinpage"
const page = await getByUIDSafe('textonlypages', 'joinpage')
---

<BaseLayout title="Join the Money Movement">
  <JoinPage page={page} client:load />
</BaseLayout>
