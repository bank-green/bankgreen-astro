---
import { MaterialsPage } from '@components/pages'
import BaseLayout from '@layouts/BaseLayout.astro'
import { getSingleSafe } from '@lib/prismic'

export const prerender = true

// Materials page uses the onepagerpage document with custom fields
const page = await getSingleSafe('onepagerpage')
---

<BaseLayout title="Materials">
  <MaterialsPage page={page} />
</BaseLayout>
