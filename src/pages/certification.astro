---
import { CertificationPage } from '@components/pages'
import BaseLayout from '@layouts/BaseLayout.astro'
import { getSingleSafe } from '@lib/prismic'

export const prerender = true

const page = await getSingleSafe('certificationpage', {
  fetchLinks: ['accordionitem.title', 'accordionitem.slices'],
})
---

<BaseLayout title="Fossil Free Certification">
  <CertificationPage page={page} client:load />
</BaseLayout>
