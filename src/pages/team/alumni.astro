---
import BaseLayout from "@layouts/BaseLayout.astro";
import { getSingleSafe } from "@lib/prismic";
import { TeamAlumniPage } from "@components/pages";

// Alumni uses the same teampage document, filtered for Alumni department
const page = await getSingleSafe("teampage", {
  fetchLinks: ["accordionitem.title", "accordionitem.slices"],
});
---

<BaseLayout title="Team Alumni">
  <TeamAlumniPage page={page} client:load />
</BaseLayout>
